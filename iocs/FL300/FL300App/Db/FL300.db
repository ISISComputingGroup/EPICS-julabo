record(stringin, "$(P)VERSION")
{
    field(DTYP, "stream")
    field(INP,  "@julaboCommon.proto getVersion $(PORT)")
    field(PINI, "YES")
}

record(stringin, "$(P)STATUS")
{
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP,  "@julaboCommon.proto getStatus $(PORT)")
}

record(ai, "$(P)TEMP:SP") 
{
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP,  "@julaboCommon.proto readSetTemp $(PORT)")
    field(PREC, "1")
    #field(EGU,  "C")
}

record(ai, "$(P)TEMP:RB") 
{
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP,  "@julaboCommon.proto readInternalTemp $(PORT)")
    field(PREC, "1")
    #field(EGU,  "C")
}

record(ao, "$(P)TEMP") 
{
    field(SCAN, "Passive")
    field(DTYP, "stream")
    field(OUT,  "@julaboCommon.proto writeTemp $(PORT)")
    #field(EGU, "C")
    field(PREC, "1")
}

record(bi, "$(P)MODE:RB") 
{
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP,  "@julaboCommon.proto readRunMode $(PORT)")
    field(ZNAM, "OFF")
    field(ONAM, "ON")
}

record(bo, "$(P)MODE") 
{
    field(SCAN, "Passive")
    field(DTYP, "stream")
    field(OUT,  "@julaboCommon.proto setRunMode $(PORT)")
    field(ZNAM, "OFF")
    field(ONAM, "ON")
}